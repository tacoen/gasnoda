<div class='grid-outline'>

<div class='g-grid'>
<div class='g-block size-100'><div class='g-content'>{{ randomwords(19)|capitalize }}</div></div> 
</div>

<div class='g-grid'>
{% for i in 1..2 %}
	<div class='g-block size-50'><div class='g-content'>{{ randomwords(9)|capitalize }} ({{i}})</div></div> 
{% endfor %}
</div>

<div class='g-grid'>
{% for i in 1..3 %}
	<div class='g-block size-33'><div class='g-content'>{{ randomwords(4)|capitalize }} ({{i}})</div></div> 
{% endfor %}
</div>

<div class='g-grid'>
{% for i in 1..4 %}
	<div class='g-block size-25'><div class='g-content'>{{ randomwords(4)|capitalize }} ({{i}})</div></div> 
{% endfor %}
</div>

<div class='g-grid'>
{% for i in 1..5 %}
	<div class='g-block size-20'><div class='g-content'>{{ randomwords(3)|capitalize }} ({{i}})</div></div> 
{% endfor %}
</div>

<div class='g-grid'>
{% for i in 1..10 %}
	<div class='g-block size-10'><div class='g-content'>({{i}})</div></div> 
{% endfor %}
</div>

</div>
